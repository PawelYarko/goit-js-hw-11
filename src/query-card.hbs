{{#each this}}
<div class="photo-card">
  <a href="{{largeImageURL}}" class="big-img">
    <img class="small-img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" />
  <div class="info">
    <div class="info-item">
      <span><b>Likes</b></span> 
      <span><b>{{likes}}</b></span>
    </div>
    <div class="info-item">
      <span><b>Views</b></span>
      <span><b>{{views}}</b></span>
    </div>
    <div class="info-item">
      <span><b>Comments</b></span>
      <span><b>{{comments}}</b></span>
    </div>
    <div class="info-item">
      <span><b>Downloads</b></span>
      <span><b>{{downloads}}</b></span>
    </div>
  </div>
  </a>
</div>
{{/each}}